{
  "name": "ids-enterprise-wc",
  "version": "0.0.0-beta.15",
  "description": "The Web Component version of the IDS Enterprise component library",
  "main": "demos/index.js",
  "directories": {
    "lib": "src",
    "test": "test",
    "doc": "doc",
    "example": "demos"
  },
  "scripts": {
    "build:dist": "npx webpack --config webpack.config.dist.js",
    "build:dist:prod": "npx webpack --config webpack.config.dist.js --mode production",
    "build:dist:prod:stats": "npx webpack --config webpack.config.dist.js --mode production",
    "build": "webpack --config webpack.config.demos.js",
    "build:stats": "webpack --config webpack.config.demos.js",
    "build:app:prod": "webpack --config webpack.config.demos.js --mode production",
    "build:verbose": "npx webpack --config webpack.config.demos.js --progress profile",
    "start": "webpack serve --live-reload --config webpack.config.demos.js",
    "start:watch": "nodemon --watch webpack.config.demos.js --exec \"npx webpack serve --config webpack.config.demos.js\"",
    "lint": "npx eslint src test demos && npx stylelint src/**/**/*.scss --config .stylelintrc.json && npm run mdlint && npx htmlhint 'demos/**/*.html' && npm run typecheck",
    "mdlint": "npx remark . -f",
    "publish:dry-run": "npm run build:dist:prod && cp -f package-dist.json dist/package.json && cp -f README.md dist && cp -f LICENSE dist && cd dist && npm publish --dry-run",
    "publish:npm": "npm run build:dist:prod && cp -f package-dist.json dist/package.json && cp -f README.md dist && cp -f LICENSE dist && cd dist && npm publish",
    "test": "PERCY_LOGLEVEL=silent npx jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:watch": "PERCY_LOGLEVEL=silent jest --watchAll --coverage --detectOpenHandles --forceExit -- func",
    "test:coverage": "PERCY_LOGLEVEL=silent jest --coverage -- func",
    "test:coverage-only-changed": "PERCY_LOGLEVEL=silent jest --coverage --onlyChanged -- func",
    "test:ui": "npx jest --coverage --runInBand --detectOpenHandles --forceExit -- func -- e2e",
    "test:ci": "npm run test:ui && npm run test:percy",
    "test:percy": "npx jest --verbose --runInBand --detectOpenHandles --forceExit -- percy",
    "test:percy:local": "npx percy exec -- npm run test:percy",
    "test:debug": "PERCY_LOGLEVEL=silent node --inspect-brk node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit",
    "test:vulnerabilities": "snyk test",
    "ncu": "npx ncu",
    "typecheck": "npx tsc --p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infor-design/enterprise-wc.git"
  },
  "bugs": {
    "url": "https://github.com/infor-design/enterprise-wc/issues"
  },
  "author": "Infor Design",
  "license": "Apache-2.0",
  "homepage": "https://design.infor.com",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/parser": "^7.15.3",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/traverse": "^7.15.0",
    "@babel/types": "^7.15.0",
    "@percy/cli": "^1.0.0-beta.65",
    "@percy/puppeteer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@wordpress/jest-puppeteer-axe": "^3.1.0",
    "autoprefixer": "^10.3.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-compat": "3.13.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsdoc": "^36.0.7",
    "eslint-plugin-wc": "^1.3.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "glob": "^7.1.7",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "htmlhint": "^0.15.1",
    "ids-identity": "4.7.1",
    "install": "^0.13.0",
    "jest": "^27.0.6",
    "jest-puppeteer": "^5.0.4",
    "loglevel": "^1.7.1",
    "mini-css-extract-plugin": "^2.2.0",
    "mutationobserver-shim": "^0.3.7",
    "nodemon": "^2.0.12",
    "pino": "^6.13.1",
    "postcss-loader": "^6.1.1",
    "puppeteer": "^10.2.0",
    "remark-cli": "^10.0.0",
    "remark-preset-lint-markdown-style-guide": "^5.0.1",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "sass-to-string": "^1.5.1",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^5.1.4",
    "typescript": "^4.3.5",
    "webpack": "5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "yaml-loader": "^0.6.0"
  }
}
