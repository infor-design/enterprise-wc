{
  "name": "ids-enterprise-wc",
  "version": "0.0.0-beta.12",
  "description": "The Web Component version of the IDS Enterprise component library",
  "main": "app/index.js",
  "directories": {
    "lib": "src",
    "test": "test",
    "doc": "doc",
    "example": "app"
  },
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --mode production",
    "build:verbose": "npx webpack --progress profile",
    "start": "webpack serve",
    "start:watch": "nodemon --watch webpack.config.js --exec \"npx webpack serve\"",
    "lint": "npx eslint src test app && npx stylelint src/**/*.scss --config .stylelintrc.json && npm run mdlint && npx htmlhint 'app/**/*.html' && npm run typecheck",
    "mdlint": "npx remark . -f",
    "publish:dry-run": "npm run build:prod && cp -f package.json dist && cp -f README.md dist && cp -f LICENSE dist && cd dist && npm publish --dry-run",
    "publish:npm": "npm run build:prod && cp -f package.json dist && cp -f README.md dist && cp -f LICENSE dist && cd dist && npm publish",
    "test": "PERCY_LOGLEVEL=silent npx jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:watch": "PERCY_LOGLEVEL=silent jest --watchAll --coverage --detectOpenHandles --forceExit -- func",
    "test:coverage": "PERCY_LOGLEVEL=silent jest --coverage -- func",
    "test:coverage-only-changed": "PERCY_LOGLEVEL=silent jest --coverage --onlyChanged -- func",
    "test:ui": "npx jest --coverage --runInBand --detectOpenHandles --forceExit -- func -- e2e",
    "test:ci": "npm run test:ui && npm run test:percy",
    "test:percy": "npx jest --verbose --runInBand --detectOpenHandles --forceExit -- percy",
    "test:percy:local": "npx percy exec -- npm run test:percy",
    "test:debug": "PERCY_LOGLEVEL=silent node --inspect-brk node_modules/.bin/jest --runInBand --detectOpenHandles --forceExit",
    "test:vulnerabilities": "snyk test",
    "ncu": "npx ncu",
    "typecheck": "npx tsc --p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infor-design/enterprise-wc.git"
  },
  "bugs": {
    "url": "https://github.com/infor-design/enterprise-wc/issues"
  },
  "author": "Infor Design",
  "license": "Apache-2.0",
  "homepage": "https://design.infor.com",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/parser": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/traverse": "^7.14.5",
    "@babel/types": "^7.14.5",
    "@percy/cli": "^1.0.0-beta.52",
    "@percy/puppeteer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@wordpress/jest-puppeteer-axe": "^3.0.5",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.6",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.3.0",
    "eslint-plugin-wc": "^1.3.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "glob": "^7.1.7",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "htmlhint": "^0.15.1",
    "ids-identity": "4.5.7",
    "install": "^0.13.0",
    "jest": "^27.0.4",
    "jest-puppeteer": "^5.0.4",
    "loglevel": "^1.7.1",
    "mini-css-extract-plugin": "^1.6.0",
    "mutationobserver-shim": "^0.3.7",
    "nodemon": "^2.0.7",
    "pino": "^6.11.3",
    "postcss-loader": "^6.1.0",
    "puppeteer": "^10.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-markdown-style-guide": "^4.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "sass-to-string": "^1.5.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-webpack-plugin": "2.1.1",
    "terser-webpack-plugin": "^5.1.3",
    "typescript": "^4.3.4",
    "webpack": "5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
