<ids-swipe-action>
              <div slot="contents">
                <ids-layout-grid cols="1" cols-md="2" gap="none" class="ids-layout-grid">
                  <ids-layout-grid-cell class="ids-layout-grid-cell">
                    <ids-text font-size="16">Tuesday, 23nd September</ids-text>
                    <ids-text font-size="14">8:10AM-2:20PM</ids-text>
                  </ids-layout-grid-cell>
                  <ids-layout-grid-cell justify-content="end" class="ids-layout-grid-cell">
                    <ids-menu-button id="actions-reveal-one" menu="actions-reveal-one-menu" aria-controls="actions-reveal-one-menu" aria-haspopup="menu">
                      <ids-icon icon="vertical-ellipsis"></ids-icon>
                      <span class="audible">Actions</span>
                    </ids-menu-button>
                    <ids-popup-menu id="actions-reveal-one-menu" target="actions-reveal-one" trigger-type="click" hidden="">
                      <ids-menu-group role="group" aria-label="Menu group containing 3 item(s)">
                        <ids-menu-item>Right Action</ids-menu-item>
                        <ids-menu-item>Left Action</ids-menu-item>
                        <ids-menu-item>Other Action</ids-menu-item>
                      </ids-menu-group>
                    </ids-popup-menu>
                  </ids-layout-grid-cell>
                </ids-layout-grid>
              </div>
              <ids-button slot="action-right" id="action-right-reveal-one" appearance="swipe-action-right" aria-hidden="true">
                <ids-icon icon="tack" size="xsmall" style="--ids-icon-height-default: 10px; --ids-icon-width-default: 10px;"></ids-icon>
                <span>Right Action</span>
              </ids-button>
            </ids-swipe-action>