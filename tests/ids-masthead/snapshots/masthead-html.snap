                    <ids-text audible="true" color-variant="alternate">Masthead logo</ids-text>
                  </ids-button>
                </ids-layout-flex-item>
                <ids-layout-flex-item>
                  <ids-text id="title" color-variant="alternate" font-size="14" font-weight="semi-bold">Infor Application</ids-text>
                </ids-layout-flex-item>
                <ids-layout-flex-item>
